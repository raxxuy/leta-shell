{
  "window": {
    "defaultHeight": 24,
    "floating": true
  },
  "layout": {
    "start": ["launcher", "workspaces", "tray"],
    "center": ["client"],
    "end": [
      "battery",
      "sound",
      "bluetooth",
      "notifications",
      "keyboard-layout",
      "clock",
      "quick-settings"
    ]
  },
  "modules": {
    "workspaces": {
      "count": 10
    },
    "clock": {
      "format": "%a %b %d  %I:%M %p",
      "fontSize": "sm"
    }
  }
}
